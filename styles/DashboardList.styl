.DashboardList
    width: 540px
    border-right: 1px solid #e5e5e5
    height: 100%
    overflow: auto
    &__heading
        display: block
        margin: 0
        background: #f7f7f7
        padding: 12px 20px
        border-bottom: 1px solid #e5e5e5
    &__list
        list-style: none
        margin: 0
        width: 100%
        padding-bottom: 10px
        background-color: #eee
        border-radius: 0 0 5px 5px
        min-height: 500px
